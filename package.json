{
  "name": "simulador-balde-bacen",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:frontend": "vite build",
    "worker": "node dist/cron.js",
    "server": "node dist/server.js",
    "start": "concurrently \"npm run worker\" \"npm run server\"",
    "dev:worker": "nodemon --exec ts-node src/cron.ts",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:frontend": "vite"
  },
  "keywords": [
    "bacen",
    "pix",
    "fichas",
    "balde",
    "pagamento"
  ],
  "author": "Bruno Vieira",
  "description": "simulador do consumo de fichas do Balde do bacen",
  "dependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.8",
    "concurrently": "^8.2.2",
    "cron": "^3.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router-bootstrap": "^0.26.3",
    "react-router-dom": "^7.8.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/typescript": "^2.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.5"
  }
}
